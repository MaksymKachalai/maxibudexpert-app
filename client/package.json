    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "parcel": "^2.6.0",
    "parcel-reporter-clean-dist": "^1.0.4",
    "posthtml-include": "^1.7.4"
  },
  "dependencies": {
    "gh-pages": "^5.0.0",
    "modern-normalize": "^1.1.0",
    "notiflix": "^3.2.6",
    "validator": "^13.9.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
